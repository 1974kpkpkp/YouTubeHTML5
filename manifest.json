{
    "manifest_version": 2,
    "minimum_chrome_version": "20",
    "content_security_policy": "default-src 'self'",
    "default_locale": "en",
    "name": "__MSG_ext_name__",
    "description": "__MSG_ext_description__",
    "version": "0.3.12",
    "icons": {
        "128": "128.png",
         "48": "48.png",
         "16": "16.png"
    },
    "permissions": [
        "storage"
    ],
    "content_scripts": [{
        "matches": [
            "*://*.youtube.com/watch*"
        ],
        "css": [
            "YouTubeHTML5.css"
        ],
        "js": [
            "YouTubeHTML5.js"
        ],
        "run_at": "document_end"
    }],
    "options_page": "options/options.html"
}
